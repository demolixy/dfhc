/**
 * 隐藏默认的checkbox
 */
input[type=checkbox] {
    visibility:hidden ;
}

.checkboxThree {
       width: 70px;
    height: 30px;
    background: #fff;
    /* margin: 20px 40px; */
    border-radius: 40px;
    position: relative;
    border: 1px solid #26ca28;
}




/**
 * Create the pill to click
 */
.checkboxThree label {
   display: block;
    width: 40px;
    height: 30px;
    border-radius: 30px;
    -webkit-transition: all .5s ease;
    -moz-transition: all .5s ease;
    -o-transition: all .5s ease;
    -ms-transition: all .5s ease;
    transition: all .5s ease;
    cursor: pointer;
    position: absolute;
    top: 0px;
    z-index: 1;
    /* left: 10px; */
    background: #e9e9e9;
}
 
/**
 * Create the checkbox event for the label
 */
.inpChecked {
    left: 30px;
    background: #26ca28!important;
}