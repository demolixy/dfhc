@charset "utf-8";
/*common */
body, a, p, ul, li, dt, dl, dd, h1, h2, h3, h4, form, input, textarea, img, select { list-style: none; margin: 0px; padding: 0px; }
body { font-family: 'Microsoft YaHei', '宋体', Arial; color: #1a2436; font-size: 0.2rem; width: 100%; height: 100%; margin: 0 auto; max-width: 640px; min-width: 320px; background-color: #f5f5f5; }
img { border: 0px none; vertical-align: top; }
body { -webkit-tap-highlight-color: rgba(0,0,0,0); -webkit-tap-highlight-color: transparent; }
input, textarea,button { font-family: 'Microsoft YaHei', '宋体', Arial; font-size: 14px; }
a { outline: none; }

:focus { outline: 0; }
a { color: #333; text-decoration: none; }
a:hover { color: #333; text-decoration: none; cursor: pointer; }
.row { width: 100%; }
.clear { clear: both; }
.clearfix:after, .clearfix:before { content: "."; display: block; height: 0; clear: both; visibility: hidden; }
/*去除苹果默认样式*/
input[type="button"], input[type="submit"], input[type="reset"] { -webkit-appearance: none; }
textarea { -webkit-appearance: none; }
input:focus, textarea:focus { outline: none; }
/*head*/
.header { width: 100%; max-width: 640px; height: 45px; background-image: -webkit-linear-gradient(top, #1a2543, #131a26 100%); background-image: linear-gradient(to bottom, #1a2543, #131a26 100%); position: fixed; top: 0px; z-index: 80; border-bottom: 1px solid #919191; border-top: 1px solid #27324e; }
.header .headCon { position: relative; padding: 1px 0; width: 100%; height: 43px; }
.header .headCon .leftBack { width: 20%; box-sizing: border-box; height: 43px; left: 0; top: 1px; position: absolute; text-align: center; font-size: 0.2rem; text-shadow: 0 2px 0 rgba(0,0,0,0.75); -webkit-text-shadow: 0 2px 0 rgba(0,0,0,0.75); color: #fff; display: block; line-height: 43px; border-right: 1px solid #0e1423; text-align:center; }
.header .headCon .leftBack div { display:inline-block; padding-left:22px; position:relative;}
.header .headCon .leftBack div i{ position:absolute; left:0; top:11px; height:21px; width:20px; background:url(../images/backIcon.png) no-repeat center center; background-size:40%;}
.header .headCon .leftBack:active { background-image: -webkit-linear-gradient(top, #172137, #111825 100%); background-image: linear-gradient(to bottom, #172137, #111825 100%); }
.header .headCon .rightQuit { width: 20%; box-sizing: border-box; height: 43px; right: 0; top: 1px; position: absolute; text-align: center; font-size: 0.2rem; border-left: 1px solid #303950; text-shadow: 0 2px 0 rgba(0,0,0,0.75); -webkit-text-shadow: 0 2px 0 rgba(0,0,0,0.75); color: #fff; display: block; line-height: 43px; }
.header .headCon .rightQuit:active { background-image: -webkit-linear-gradient(top, #172137, #111825 100%); background-image: linear-gradient(to bottom, #172137, #111825 100%); }
.header .headCon span { display: block; position: relative; margin: 0 20%; line-height: 43px; border-left: 1px solid #303950; border-right: 1px solid #0e1423; text-align: center; line-height: 43px; box-sizing: border-box; font-size: 0.28rem; height: 43px; color: #fff; text-shadow: 0 2px 0 rgba(0,0,0,0.75); -webkit-text-shadow: 0 2px 0 rgba(0,0,0,0.75); }

/*底部悬浮*/
.bottom-float { position: fixed; z-index: 90; bottom: 0; left: 0; background-color: #e9e9e9; display: -webkit-box; display: -webkit-flex; display: -ms-flexbox; display: flex; width: 100%; border-top: 1px solid #bdbdbd; }
.bottom-float a { display: block; font-size: 14px; text-align: center; color: #5a5a5a; padding: 10px 0; }
.flex1 { -webkit-box-flex: 1; -webkit-flex: 1; -ms-flex: 1; box-flex: 1; flex: 1; overflow: hidden; display: block; }
.flex2 { -webkit-box-flex: 0 0 0; -webkit-flex: 0 0 0; -ms-flex: 0 0 0; box-flex: 0 0 0; flex: 0 0 0; border-right: 1px solid #d6d6d6; border-left: 1px solid #f6f6f6; overflow: hidden; display: block; margin: 10px 0; height: 24px; }
.flex3 { -webkit-box-flex: 2; -webkit-flex: 2; -ms-flex:2; box-flex:2; flex:2; overflow: hidden; display: block; }
.bottom-float a span { display: inline-block; position: relative; padding-left: 26px; height: 100%; line-height: 24px; }
.bottom-float a span i { position: absolute; width: 22px; height: 24px; left: 0; font-size: 16px; top: 0; color: #5a5a5a; text-align: center; line-height: 24px; }
.bottom-float a.on { background-image: -webkit-linear-gradient(top, #222934, #29334e 100%); background-image: linear-gradient(to bottom, #222934, #29334e 100%); }
.bottom-float a.on i { color: #fff; }
.bottom-float a.on { color: #fff; }

/*登录*/
.login{ width:100%; background:url(../../../images/android/images/login_bg.png) no-repeat top left; background-size:100% auto; position:relative; font-size:0.24rem;}
.login .loginCon{ padding:65% 0 20px 0; overflow:hidden; width:70%; margin:0 auto;}
.login .loginCon .loginArea{
 padding:5px 0;
}
.loginArea .dlItem{ 
     width:100%; height:44px; 
     line-height:44px; padding-left:50px; 
     position:relative; box-sizing:border-box;
     border: 1px solid #fff;
     border-radius: 5px; }
.loginArea .dlItem:first-child{margin-bottom: 20px;}     
.loginArea .dlItem i{ 
    position:absolute;
    left:10px; 
    font-size:0.4rem;
    height:24px; 
    width:24px; 
    padding:1px; 
    text-align:center;
    line-height:24px; 
    color:rgba(255, 255, 255, 0.70); 
    top:7px;  
    display:block;}
.loginArea .dlItem .dlInput{ 
    width:100%; 
    background-color:rgba(255, 255, 255, 0); 
    border:0px none;
    color: #fff;
    box-sizing:border-box;

}
.copyright{ position:absolute; bottom:15px; left:0; width:100%; text-align:center; color:#fff;}
.loginCon .remenber{ padding:8% 0; float:left; margin-right: 5%;}
.remenber label{color:#fff;}


.loginCon .remenber input[type=checkbox]{ vertical-align:middle; margin:0 3px 3px 0;}
.loginSubmit .loginBtn {
    width: 100%;
    height: 44px;
    line-height: 42px;
    text-align: center;
    font-size: 0.3rem;
    color: #fff;
    text-align: center;
    border-radius: 6px;
    background-image: -webkit-linear-gradient(top, #3366cc, #0179c5 100%);
    background-image: linear-gradient(to bottom, #3366cc, #0179c5 100%);
    border: 0px none;
    border-top: 1px solid #3366cc;
    
}
.loginSubmit .loginBtn:active{background-image: -webkit-linear-gradient(top, #2d9ee6, #026db1 100%);
background-image: linear-gradient(to bottom, #2d9ee6, #026db1 100%); border-top:1px solid #4dbafe;}
/*主界面*/
.indexWrap{ background:#7cd9cb url(../images/main_bg.png) no-repeat bottom left; background-size:100% auto; font-size:0.24rem; background-attachment:fixed;}
.indexCon{ padding-top:70px;}
.indexCon .userName{ padding:0 0 5px 20px; color:#fff; line-height:20px;}
.indexCon .menuCon{ width:100%;font-size:0.3rem;}
.menuCon .item{ padding-top:5%; float:left; width:50%; display:inline-block; text-align:center; line-height:30px; color:#161f34;}
.menuCon .item p{ display:block;}
.menuCon .item .menuIcon{height:1.5rem; width:100%; background-position:top center; background-repeat:no-repeat;background-size:contain; position:relative;}
.menuCon .item .menuIcon span{ position:absolute; width:0.8rem; height:0.8rem; line-height:2.0; text-align:center; top:-0.4rem; left:65%;border-radius:50%; background-color:#d80000; box-shadow:0 3px 0 rgba(0,0,0,0.26); color:#fff; font-size:0.4rem;display:block;}
 @media screen and (min-height : 569px) {
.menuCon .item .menuIcon{height:1.7rem; width:100%; background-position:top center; background-repeat:no-repeat;background-size:contain;}
.menuCon .item{ padding-top:7%; float:left; width:50%; display:inline-block; text-align:center; line-height:30px;}
}
 @media screen and (max-height : 650px) and (min-height : 570px) {
.menuCon .item .menuIcon{height:1.8rem; width:100%; background-position:top center; background-repeat:no-repeat;background-size:contain; margin-bottom:2%;}
.menuCon .item{ padding-top:8%; float:left; width:50%; display:inline-block; text-align:center; line-height:30px;}
}
 @media screen and (min-height : 651px) {
.menuCon .item .menuIcon{height:2rem; width:100%; background-position:top center; background-repeat:no-repeat;background-size:contain;margin-bottom:2%;}
.menuCon .item{ padding-top:8%; float:left; width:50%; display:inline-block; text-align:center; line-height:30px;}
}
