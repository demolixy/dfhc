  <sql id="where">
   <trim prefix="where" prefixOverrides="and|or">  
      <if test="id != null and id != ''">
        and BUS_GATE.ID = #{id}
      </if>
      <if test="status != null and status != ''">
        and BUS_GATE.STATUS = #{status}
      </if>
      <if test="startTime != null and startTime != ''">
        and to_char(BUS_GATE.START_TIME,'YYYY-MM-DD') = #{startTime}
      </if>
      <if test="endTime != null and endTime != ''">
        and to_char(BUS_GATE.END_TIME,'YYYY-MM-DD') = #{endTime}
      </if>
      <if test="isEnabled != null and isEnabled != ''">
        and BUS_GATE.IS_ENABLED = #{isEnabled}
      </if>
      <if test="delete_flag != null and delete_flag != ''">
        and nvl(BUS_GATE.DELETE_FLAG,'0') = #{delete_flag}
      </if>
      </trim>
  </sql>

