package com.dfhc.base.price.vo;

import java.io.Serializable;

/**
 * 用户记录首页车辆数量的
 * @author dfhc
 *
 */
public class TodayCarTabVo implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	/**
	 * 今日签到车数
	 */
	private int todayCount;
	/**
	 * 今日过磅车数
	 */
	private int isPoundCarNum;
	/**
	 * 今日一过磅车数
	 */
	private int isPounedCarNum;
	/**
	 * 今日装车数
	 */
	private int isLoadingCarNum;
	/**
	 * 今日已装车数
	 */
	private int isLoadedCarNum;
	/**
	 * @return the todayCount
	 */
	public int getTodayCount() {
		return todayCount;
	}
	/**
	 * @param todayCount the todayCount to set
	 */
	public void setTodayCount(int todayCount) {
		this.todayCount = todayCount;
	}
	/**
	 * @return the isPoundCarNum
	 */
	public int getIsPoundCarNum() {
		return isPoundCarNum;
	}
	/**
	 * @param isPoundCarNum the isPoundCarNum to set
	 */
	public void setIsPoundCarNum(int isPoundCarNum) {
		this.isPoundCarNum = isPoundCarNum;
	}
	/**
	 * @return the isPounedCarNum
	 */
	public int getIsPounedCarNum() {
		return isPounedCarNum;
	}
	/**
	 * @param isPounedCarNum the isPounedCarNum to set
	 */
	public void setIsPounedCarNum(int isPounedCarNum) {
		this.isPounedCarNum = isPounedCarNum;
	}
	/**
	 * @return the isLoadingCarNum
	 */
	public int getIsLoadingCarNum() {
		return isLoadingCarNum;
	}
	/**
	 * @param isLoadingCarNum the isLoadingCarNum to set
	 */
	public void setIsLoadingCarNum(int isLoadingCarNum) {
		this.isLoadingCarNum = isLoadingCarNum;
	}
	/**
	 * @return the isLoadedCarNum
	 */
	public int getIsLoadedCarNum() {
		return isLoadedCarNum;
	}
	/**
	 * @param isLoadedCarNum the isLoadedCarNum to set
	 */
	public void setIsLoadedCarNum(int isLoadedCarNum) {
		this.isLoadedCarNum = isLoadedCarNum;
	}
	public TodayCarTabVo() {
		super();
	}
	public TodayCarTabVo(int todayCount, int isPoundCarNum, int isPounedCarNum,
			int isLoadingCarNum, int isLoadedCarNum) {
		super();
		this.todayCount = todayCount;
		this.isPoundCarNum = isPoundCarNum;
		this.isPounedCarNum = isPounedCarNum;
		this.isLoadingCarNum = isLoadingCarNum;
		this.isLoadedCarNum = isLoadedCarNum;
	}
	
	
	

}
