package com.dfhc.pub;

/**
 * 文件锁异常
 * @author longsebo
 */
public class FileLockException extends RuntimeException {

	/**
	 * 
	 */
	private static final long serialVersionUID = -7591792535048363329L;

	public FileLockException() {
	}

	public FileLockException(String message) {
		super(message);
	}

	public FileLockException(String message, Throwable t) {
		super(message, t);
	}
}
