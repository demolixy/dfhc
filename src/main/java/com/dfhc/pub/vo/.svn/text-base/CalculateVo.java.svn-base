package com.dfhc.pub.vo;

import java.math.BigDecimal;

/**
 * 结算单操作vo
 * @author dfhc
 *
 */
public class CalculateVo {
	
	/**
	 * 计划发生额度
	 */
	private BigDecimal oldChangeAmt;
	/**
	 * 实际发生金额
	 */
	private BigDecimal actualChangeAmt;
	
	
	/**
	 * 付款人id
	 */
	private String  payerId;
	
	/**
	 * 单据id
	 */
	private String billsId;
	
	/**
	 * 业务类型
	 */
	private String businessType;
	/**
	 * 操作人id
	 */
	
	private String operaterId;
	
	/**
	 * 操作人名称
	 */
	
	private String operaterName;
	
	/**
	 * 操作ip
	 * 
	 */
	
	private String ip;
	

	/**
	 * 备注
	 */
	private String remark;
	

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	public BigDecimal getOldChangeAmt() {
		return oldChangeAmt;
	}

	public void setOldChangeAmt(BigDecimal oldChangeAmt) {
		this.oldChangeAmt = oldChangeAmt;
	}

	public BigDecimal getActualChangeAmt() {
		return actualChangeAmt;
	}

	public void setActualChangeAmt(BigDecimal actualChangeAmt) {
		this.actualChangeAmt = actualChangeAmt;
	}

	public String getBillsId() {
		return billsId;
	}

	public void setBillsId(String billsId) {
		this.billsId = billsId;
	}

	public String getBusinessType() {
		return businessType;
	}

	public void setBusinessType(String businessType) {
		this.businessType = businessType;
	}

	public String getOperaterId() {
		return operaterId;
	}

	public void setOperaterId(String operaterId) {
		this.operaterId = operaterId;
	}

	public String getOperaterName() {
		return operaterName;
	}

	public void setOperaterName(String operaterName) {
		this.operaterName = operaterName;
	}

	public String getIp() {
		return ip;
	}

	public void setIp(String ip) {
		this.ip = ip;
	}

	public String getPayerId() {
		return payerId;
	}

	public void setPayerId(String payerId) {
		this.payerId = payerId;
	}
	
	/**
	 * 
	 * @param oldChangeAmt：原计划发生金额
	 * @param actualChangeAmt：实际发生金额
	 * @param payerId：账户持有人id
	 * @param billsId:单据id
	 * @param businessType：单据类型
	 * @param operaterId：操作人id
	 * @param operaterName：操作人名称
	 * @param ip：操作地ip
	 * @param remark:业务类型备注
	 */

	public CalculateVo(BigDecimal oldChangeAmt, BigDecimal actualChangeAmt,
			String payerId, String billsId, String businessType,
			String operaterId, String operaterName, String ip,String remark) {
		super();
		this.oldChangeAmt = oldChangeAmt;
		this.actualChangeAmt = actualChangeAmt;
		this.payerId = payerId;
		this.billsId = billsId;
		this.businessType = businessType;
		this.operaterId = operaterId;
		this.operaterName = operaterName;
		this.ip = ip;
		this.remark=remark;
	}

	public CalculateVo() {
		super();
	}

	
	
	
	
	
	

}
